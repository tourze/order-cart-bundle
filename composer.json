{
  "name": "tourze/order-cart-bundle",
  "description": "Shopping cart management bundle for e-commerce systems",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/security-http": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/biz-user-bundle": "0.0.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-indexed-bundle": "1.0.*",
    "tourze/doctrine-snowflake-bundle": "1.0.*",
    "tourze/doctrine-timestamp-bundle": "1.0.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/json-rpc-core": "1.0.*",
    "tourze/json-rpc-lock-bundle": "0.1.*",
    "tourze/lock-service-bundle": "1.0.*",
    "tourze/product-core-bundle": "0.0.*",
    "tourze/product-service-contracts": "0.0.*",
    "tourze/stock-manage-bundle": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "symfony/validator": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\OrderCartBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\OrderCartBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}